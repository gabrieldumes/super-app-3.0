<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.superapp30">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_super_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_super_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SuperApp30">
        <activity android:name=".activity.VerNoticiaActivity"></activity>
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.SuperApp30.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>